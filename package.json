{
  "name": "navbar",
  "version": "2.1.0",
  "description": "A tiny library to create nav elements that smart update on scroll to keep the correct item active. Fast, supports older browsers, and has no dependencies.",
  "main": "navbar.umd.js",
  "jsnext:main": "navbar.es6.js",
  "module": "navbar.es6.js",
  "homepage": "https://qubyte.github.io/navbar/",
  "scripts": {
    "build-umd": "rollup -f umd -n navbar navbar.es6.js > navbar.umd.js",
    "test": "eslint . && testem ci --launch firefox"
  },
  "repository": "github:qubyte/navbar",
  "keywords": [
    "navigation",
    "tracking"
  ],
  "author": "Mark Stanley Everitt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qubyte/navbar/issues"
  },
  "files": [
    "navbar.umd.js",
    "navbar.es6.js"
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.0.0",
    "eslint-config-qubyte": "~2.0.0",
    "mocha": "~6.2.0",
    "rollup": "~1.18.0",
    "sinon": "^7.2.4",
    "testem": "~2.17.0"
  }
}
